# Example .env file for Auto-GPT GitHub Agent
# Copy this to .env and fill in your values

# Optional: OpenAI API key for enhanced analysis
# OPENAI_API_KEY=sk-...

# Optional: GitHub token for accessing private repositories
# GITHUB_TOKEN=ghp_...

# Agent settings
MAX_ITERATIONS=10
VERBOSE=true

# Output settings
OUTPUT_DIR=./output

# Repository cloning settings
CLONE_DEPTH=1
